    "# 2. implement queue using linked list\n",
    "# rear --> front\n",
    "class Queue:\n",
    "    def __init__(self):\n",
    "        self.head = None\n",
    "        \n",
    "    def isEmpty(self):\n",
    "        return self.head == None\n",
    "    \n",
    "    def size(self):\n",
    "        '''return length of queue\n",
    "        O(n)'''\n",
    "        cur = self.head\n",
    "        count = 0\n",
    "        while cur:\n",
    "            count += 1\n",
    "            cur = cur.next\n",
    "        return count\n",
    "    \n",
    "    def enqueue(self, item):\n",
    "        '''add item to rear\n",
    "        O(n)'''\n",
    "        newNode = Node(item)\n",
    "        if self.head == None:\n",
    "            self.head = newNode\n",
    "        else:\n",
    "            newNode.next = self.head\n",
    "            self.head = newNode\n",
    "            \n",
    "    def dequeue(self):\n",
    "        '''remove front item from queue\n",
    "        O(n0)'''\n",
    "        if self.head == None:\n",
    "            raise Exception('queue is empty!')\n",
    "        if self.head.next == None:\n",
    "            val = self.head.data\n",
    "            self.head = None\n",
    "            return val\n",
    "        cur = self.head\n",
    "        while cur.next.next:\n",
    "            cur = cur.next\n",
    "        # store last node's value\n",
    "        lastVal = cur.next.data\n",
    "        # remove last node\n",
    "        cur.next = None\n",
    "        return lastVal\n",
    "    \n",
    "    def __repr__(self):\n",
    "        nodes = []\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            nodes.append(str(cur.data))\n",
    "            cur = cur.next\n",
    "        return '->'.join(nodes)"
   ]
