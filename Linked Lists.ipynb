    "# Linked List Implementation\n",
    "class LinkedList:\n",
    "    def __init__(self):\n",
    "        self.head = None\n",
    "        \n",
    "    def isEmpty(self):\n",
    "        '''check if linked list is empty.'''\n",
    "        return self.head == None\n",
    "    \n",
    "    def add(self, item):\n",
    "        '''add a new item at the beginning (head)\n",
    "        O(1)\n",
    "        '''\n",
    "        # create a temp node for item\n",
    "        temp = Node(item)\n",
    "        # set temp to refer to old head\n",
    "        temp.next = self.head\n",
    "        # set temp to be new head\n",
    "        self.head = temp\n",
    "        \n",
    "    def size(self):\n",
    "        '''\n",
    "        get size of a linked list (#nodes)\n",
    "        O(n)\n",
    "        traverse linked list and count #nodes\n",
    "        '''\n",
    "        cur = self.head\n",
    "        count = 0\n",
    "        while cur != None:\n",
    "            count += 1\n",
    "            cur = cur.next\n",
    "        return count\n",
    "    \n",
    "    def search(self, key):\n",
    "        '''\n",
    "        search for 'key' in linked list\n",
    "        traverse ll to find 'key'\n",
    "        re: T/F\n",
    "        O(n)\n",
    "        '''\n",
    "        cur = self.head\n",
    "        while cur != None:\n",
    "            if cur.data == key:\n",
    "                return True    # found\n",
    "            cur = cur.next\n",
    "        # not found\n",
    "        return False\n",
    "    \n",
    "    def remove(self, key):\n",
    "        '''remove key from the linked list\n",
    "        O(n)\n",
    "        '''\n",
    "        # if ll is empty, raise error\n",
    "        if self.head is None:\n",
    "            raise Exception(\"Linked list is empty!\")\n",
    "        \n",
    "        # if head node itself holds key to be removed,\n",
    "        # simply set a new head\n",
    "        if self.head.data == key:\n",
    "            self.head = self.head.next\n",
    "            return\n",
    "        \n",
    "        # traverse linked list for 'key' to remove\n",
    "        prev = None\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            # key found, break loop\n",
    "            if cur.data == key:\n",
    "                prev.next = cur.next\n",
    "                return\n",
    "            # key not found yet, move prev and cur one node ahead\n",
    "            prev = cur\n",
    "            cur = cur.next\n",
    "        \n",
    "        # cur is None, key not found\n",
    "        raise Exception('Key not found in ll!')\n",
    "        \n",
    "    def .............................................................................................................(self, item):\n",
    "        '''append an item to the end of ll.\n",
    "        O(n)\n",
    "        '''\n",
    "        # create a node for item (by default, node points to None)\n",
    "        newNode = Node(item)\n",
    "        \n",
    "        # if ll is empty, simply set head to be new node\n",
    "        if self.head == None:\n",
    "            self.head = newNode\n",
    "        \n",
    "        # traverse to the end of ll and append new node\n",
    "        cur = self.head\n",
    "        while cur.next:\n",
    "            cur = cur.next\n",
    "        cur.next = newNode\n",
    "    \n",
    "    def index(self, key):\n",
    "        '''return position of item in ll.\n",...............................................................
    "        O(n)\n",
    "        '''\n",
    "        # if ll is empty, raise error\n",
    "        if self.head == None:\n",
    "            raise Exception(\"Linked list is empty!\")\n",
    "        # traverse ll to find key\n",
    "        pos = 0\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            if cur.data == key:\n",
    "                return pos    # found\n",
    "            # else, move to next node\n",
    "            cur = cur.next\n",
    "            pos += 1\n",
    "        return -1    # not found\n",
    "    \n",
    "    def popLastNode(self):\n",
    "        '''remove and return the last item of ll.\n",
    "        O(n)\n",
    "        '''\n",
    "        # if ll is empty, cannot pop\n",
    "        if self.head == None:\n",
    "            raise Exception(\"Linked list is empty\")\n",
    "        # if there is only 1 node, set ll to empty\n",
    "        if self.head.next == None:\n",
    "            self.head = None\n",
    "        # otherwise, traverse and remove last node\n",
    "        cur = self.head\n",
    "        while cur.next.next:\n",
    "            cur = cur.next\n",
    "        lastVal = cur.next.data\n",
    "        cur.next = None\n",
    "        return lastVal\n",
    "    \n",
    "    def pop(self, pos=0):\n",
    "        '''remove and return the item at position pos.\n",
    "        O(n)\n",
    "        '''\n",
    "        # invalid pos\n",
    "        if pos < 0:\n",
    "            raise Exception('Invalid position!')\n",
    "        # ll is empty, raise error\n",
    "        if self.head == None:\n",
    "            raise Exception(\"Linked list is empty!\")\n",
    "        # ll has 1 node, set ll to empty and return node val\n",
    "        if self.head.next == None:\n",
    "            val = self.head.data\n",
    "            self.head = None\n",
    "            return val\n",
    "        # pos > ll size, raise error\n",
    "        if pos >= self.size():\n",
    "            raise IndexError(\"Index out of range!\")\n",
    "        # pos = size - 1, append new node\n",
    "        if pos == self.size() - 1: # CAN COMBINE THIS CASE W LAST CASE\n",
    "            return self.popLastNode()\n",
    "        # otherwise, traverse ll to pos\n",
    "        prev = None\n",
    "        cur = self.head\n",
    "        idx = 0\n",
    "        while cur:\n",
    "            if idx == pos:\n",
    "                val = cur.data\n",
    "                prev.next = cur.next\n",
    "                return val\n",
    "            idx += 1\n",
    "            prev = cur\n",
    "            cur = cur.next\n",
    "    \n",
    "    def insert(self, item, pos=0):\n",
    "        '''insert an item at position pos.\n",
    "        O(n)\n",
    "        '''\n",
    "        # invalid pos\n",
    "        if pos < 0:\n",
    "            raise Exception(\"Invalid position\")\n",
    "        # ll is empty, raise error\n",
    "        if self.head == None:\n",
    "            raise Exception(\"Linked list is empty!\")\n",
    "        # add new node at the beginning\n",
    "        elif pos == 0:\n",
    "            self.add(item)\n",
    "        # append new node to the end\n",
    "        elif pos == self.size() - 1:\n",
    "            self.append(item)\n",
    "        # pos > size, raise error\n",
    "        elif pos >= self.size():\n",
    "            raise IndexError(\"Index out of range!\")\n",
    "        # traverse ll to pos\n",
    "        else:\n",
    "            prev = None\n",
    "            cur = self.head\n",
    "            idx = 0\n",
    "            while idx < pos:\n",
    "                prev = cur\n",
    "                cur = cur.next\n",
    "                idx += 1\n",
    "            newNode = Node(item)\n",
    "            prev.next = newNode\n",
    "            newNode.next = cur\n",
    "    \n",
    "    def __repr__(self):\n",
    "        '''linked list representation\n",
    "        O(n)\n",
    "        '''\n",
    "        nodes = []\n",
    "        cur = self.head\n",
    "        while cur != None:\n",
    "            nodes.append(str(cur.data))\n",
    "            cur = cur.next\n",
    "        nodes.append(\"None\")\n",
    "        return '->'.join(nodes)"
   ]
