    "# 1. implement a stack using linked list\n",
    "class Stack:\n",
    "    # head is None by default\n",
    "    def __init__(self):\n",
    "        self.head = None\n",
    "        \n",
    "    def isEmpty(self):\n",
    "        '''check if stack is empty'''\n",
    "        return self.head == None\n",
    "    \n",
    "    def push(self, item):\n",
    "        '''push an item on stack'''\n",
    "        if self.head == None:\n",
    "            self.head = Node(item)\n",
    "        else:\n",
    "            newNode = Node(item)\n",
    "            newNode.next = self.head\n",
    "            self.head = newNode\n",
    "            \n",
    "    def pop(self):\n",
    "        '''remove and return last item on stack (current head)'''\n",
    "        if self.isEmpty():\n",
    "            raise Exception('stack is empty!')\n",
    "        popped = self.head\n",
    "        self.head = self.head.next\n",
    "        popped.next = None\n",
    "        return popped.data\n",
    "    \n",
    "    def peek(self):\n",
    "        '''return top of stack'''\n",
    "        if self.head == None:\n",
    "            raise Exception('stack is empty!')\n",
    "        return self.head.data\n",
    "    \n",
    "    def __repr__(self):\n",
    "        '''stack representation'''\n",
    "        if self.head == None:\n",
    "            raise Exception('stack underflow')\n",
    "        nodes = []\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            nodes.append(str(cur.data))\n",
    "            cur = cur.next\n",
    "        return '->'.join(nodes)"
   ]
