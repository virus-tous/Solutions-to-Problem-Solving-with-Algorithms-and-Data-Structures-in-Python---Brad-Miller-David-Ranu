    "# Node implementation\n",
    "class Node:\n",
    "    def __init__(self, initData):\n",
    "        self.data = initData\n",
    "        self.next = None\n",
    "        \n",
    "    def __repr__(self):\n",
    "        '''node representation'''\n",
    "        return str(self.data)\n",
    "    \n",
    "    def getData(self):\n",
    "        return self.data\n",
    "    \n",
    "    def getNext(self):\n",
    "        return self.next\n",
    "    \n",
    "    def setData(self, newData):\n",
    "        self.data = newData\n",
    "        \n",
    "    def setNext(self, newNext):\n",
    "        self.next = newNext"
