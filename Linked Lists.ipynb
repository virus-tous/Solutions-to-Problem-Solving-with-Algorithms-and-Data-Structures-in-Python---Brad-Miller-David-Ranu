    "# Ordered List Implementation\n",
    "# in ascending order\n",
    "class OrderedList:\n",
    "    def __init__(self):\n",
    "        self.head = None\n",
    "    \n",
    "    def isEmpty(self):\n",
    "        '''check if list is empty\n",
    "        O(1)'''\n",
    "        return self.head == None\n",
    "    \n",
    "    def size(self):\n",
    "        '''return #nodes\n",
    "        O(n)'''\n",
    "        cur = self.head\n",
    "        count = 0\n",
    "        while cur:\n",
    "            count += 1\n",
    "            cur = cur.next\n",
    "        return count\n",
    "    \n",
    "    def search(self, key):\n",
    "        '''check for the occurrence of key in list\n",
    "        O(n)'''\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ordered list is empty!\")\n",
    "        if key < self.head.data:\n",
    "            return False\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            if cur.data == key:\n",
    "                return True\n",
    "            if cur.data > key:\n",
    "                return False\n",
    "            cur = cur.next\n",
    "        # key is greater than all items\n",
    "        return False        \n",
    "    \n",
    "    def add(self, item):\n",
    "        '''add item to list while maintaining order\n",
    "        O(n)'''\n",
    "        newNode = Node(item)\n",
    "        # list is empty, set head to new node\n",
    "        if self.head == None:\n",
    "            self.head = newNode\n",
    "            return\n",
    "        # new node is smallest, add at front\n",
    "        if item <= self.head.data:\n",
    "            newNode.next = self.head\n",
    "            self.head = newNode\n",
    "            return\n",
    "        \n",
    "        # traverse list to find proper position\n",
    "        prev = None\n",
    "        cur = self.head\n",
    "        while cur and cur.data < item:\n",
    "            prev = cur\n",
    "            cur = cur.next\n",
    "        # insert new node at proper position\n",
    "        prev.next = newNode\n",
    "        newNode.next = cur\n",
    "    \n",
    "    def remove(self, key):\n",
    "        '''remove item from list\n",
    "        O(n)'''\n",
    "        # list is empty, raise error\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ordered list is empty!\")\n",
    "        # head holds key, set new head\n",
    "        if self.head.data == key:\n",
    "            self.head = self.head.next\n",
    "            return\n",
    "        \n",
    "        # otherwise, traverse list to find key\n",
    "        prev = None\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            if cur.data == key:\n",
    "                prev.next = cur.next\n",
    "                return\n",
    "            #key not found yet, move prev and cur 1 node ahead\n",
    "            prev = cur\n",
    "            cur = cur.next\n",
    "        # key not present, cur is None\n",
    "        raise Exception(\"Item not found!\")\n",
    "    \n",
    "    \n",
    "    def index(self, key):\n",
    "        '''return position of key in list\n",
    "        O(n)'''\n",
    "        if self.head == None:\n",
    "            raise Exception(\"Ordered list is empty!\")\n",
    "        cur = self.head\n",
    "        pos = 0\n",
    "        while cur:\n",
    "            if cur.data == key:\n",
    "                return pos\n",
    "            if cur.data > key:\n",
    "                return -1\n",
    "            cur = cur.next\n",
    "            pos += 1\n",
    "        return -1    # key not present\n",
    "    \n",
    "    def popLastNode(self):\n",
    "        '''remove and return last node\n",
    "        O(n)'''\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ordered list is empty!\")\n",
    "        if self.head.next == None:\n",
    "            val = self.head.data\n",
    "            sefl.head = None\n",
    "            return val\n",
    "        cur = self.head\n",
    "        while cur.next.next:\n",
    "            cur = cur.next\n",
    "        lastVal = cur.next.data\n",
    "        cur.next = None\n",
    "        return lastVal\n",
    "    \n",
    "    def pop(self, pos=0):\n",
    "        '''remove and return item at position pos\n",
    "        O(n)'''\n",
    "        if pos < 0:\n",
    "            raise Exception(\"Invalid position!\")\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ordered list is empty!\")\n",
    "        if pos >= self.size():\n",
    "            raise IndexError(\"index out of range!\")\n",
    "        if pos == 0:\n",
