    "# linked list implementation using only 1 reference\n",
    "class LinkedList2:\n",
    "    def __init__(self):\n",
    "        self.head = None\n",
    "        \n",
    "    def __repr__(self):\n",
    "        '''linked list representation\n",
    "        O(n)'''\n",
    "        nodes = []\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            nodes.append(str(cur.data))\n",
    "            cur = cur.next\n",
    "        nodes.append('None')\n",
    "        return '->'.join(nodes)\n",
    "    \n",
    "    def isEmpty(self):\n",
    "        '''check if ll is empty\n",
    "        O(1)'''\n",
    "        return self.head == None\n",
    "    \n",
    "    def size(self):\n",
    "        '''return #nodes in ll\n",
    "        O(n)'''\n",
    "        cur = self.head\n",
    "        count = 0\n",
    "        while cur:\n",
    "            count += 1\n",
    "            cur = cur.next\n",
    "        return count\n",
    "    \n",
    "    def search(self, key):\n",
    "        '''check the occurrence of key in ll\n",
    "        O(n)'''\n",
    "        # ll is empty, raise error\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ll is empty!\")\n",
    "        # otherwise, traverse ll to find key\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            if cur.data == key:\n",
    "                return True\n",
    "            cur = cur.next\n",
    "        # cur is None > key not present\n",
    "        return False\n",
    "    \n",
    "    def add(self, item):\n",
    "        '''add an item to the head of ll\n",
    "        O(1)'''\n",
    "        newNode = Node(item)\n",
    "        newNode.next = self.head\n",
    "        self.head = newNode\n",
    "        \n",
    "    def append(self, item):\n",
    "        '''append an item to the end of ll\n",
    "        O(n)'''\n",
    "        newNode = Node(item)\n",
    "        # ll is empty, set head to new node\n",
    "        if self.head == None:\n",
    "            self.head = newNode\n",
    "        # otherwise, traverse to the end of ll and append new node\n",
    "        cur = self.head\n",
    "        while cur.next:\n",
    "            cur = cur.next\n",
    "        cur.next = newNode\n",
    "        \n",
    "    def remove(self, key):\n",
    "        '''remove item from ll\n",
    "        2 pointers\n",
    "        O(n)\n",
    "        '''\n",
    "        # ll empty, raise error\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ll is empty!\")\n",
    "        # head itself holds key, move head 1 node ahead\n",
    "        if self.head.data == key:\n",
    "            self.head = self.head.next\n",
    "            return\n",
    "        # otherwise, traverse ll to find key\n",
    "        prev = None\n",
    "        cur = self.head\n",
    "        while cur:\n",
    "            # key found, make prev point to cur.next\n",
    "            if cur.data == key:\n",
    "                prev.next = cur.next\n",
    "                return\n",
    "            # key not found yet, move prev and cur 1 node ahead\n",
    "            prev = cur\n",
    "            cur = cur.next\n",
    "        # cur is None > key not present\n",
    "        raise Exception('key not present in ll!')\n",
    "        \n",
    "    def index(self, key):\n",
    "        '''return position of key in ll\n",
    "        O(n)'''\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ll is empty!\")\n",
    "        cur = self.head\n",
    "        pos = 0\n",
    "        while cur:\n",
    "            # key found, return pos\n",
    "            if cur.data == key:\n",
    "                return pos\n",
    "            # key not found yet, move cur 1 node ahead\n",
    "            cur = cur.next\n",
    "            pos += 1\n",
    "        # key not present\n",
    "        return -1\n",
    "    \n",
    "    def insert(self, item, pos=0):\n",
    "        '''insert item to ll at position pos\n",
    "        O(n)'''\n",
    "        if pos < 0:\n",
    "            raise IndexError('invalid index!')\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ll is empty!\")\n",
    "        if pos == 0:\n",
    "            self.add(item)\n",
    "            return\n",
    "        if pos >= self.size():\n",
    "            raise IndexError('index out of range!')\n",
    "            \n",
    "        # otherwise, traverse ll upto (pos - 1) nodes\n",
    "        newNode = Node(item)\n",
    "        cur = self.head\n",
    "        while pos > 1:\n",
    "            pos -= 1\n",
    "            cur = cur.next\n",
    "            \n",
    "        # insert new node between cur(pos-1) and cur.next(pos+1)\n",
    "        newNode.next = cur.next\n",
    "        cur.next = newNode\n",
    "        \n",
    "    def popLastNode(self):\n",
    "        '''remove and return last item\n",
    "        O(n)'''\n",
    "        # ll empty > raise error\n",
    "        if self.head == None:\n",
    "            raise Exception('ll is empty!')\n",
    "        # ll has 1 node > empty ll and return this node\n",
    "        if self.head.next == None:\n",
    "            val = self.head.data\n",
    "            self.head = None\n",
    "            return val\n",
    "        # otherwise, traverse ll upto 2nd-2-last item\n",
    "        cur = self.head\n",
    "        while cur.next.next:\n",
    "            cur = cur.next\n",
    "        lastVal = cur.next.data\n",
    "        cur.next = None\n",
    "        return lastVal\n",
    "    \n",
    "    def pop(self, pos=0):\n",
    "        '''remove and return item at pos'''\n",
    "        if pos < 0:\n",
    "            raise IndexError('invalid index!')\n",
    "        if pos >= self.size():\n",
    "            raise IndexError('index out of range!')\n",
    "        if self.head == None:\n",
    "            raise Exception('ll is empty!')\n",
    "        if pos == 0:\n",
    "            self.head = self.head.next\n",
    "        if pos == self.size() - 1:\n",
    "            return self.popLastNode()\n",
    "        # otherwise, traverse ll upto (pos - 1) nodes\n",
    "        cur = self.head\n",
    "        while pos > 1:\n",
    "            pos -= 1\n",
    "            cur = cur.next\n",
    "    \n",
    "        val = cur.next.data\n",
    "        cur.next = cur.next.next\n",
    "        return val\n",
    "    \n",
    "    def mySlice(self, start, end):\n",
    "        '''return a slice of ll [start, end)\n",
    "        O(n)'''\n",
    "        if start < 0:\n",
    "            start = 0\n",
    "        if end > self.size():\n",
    "            end = self.size()\n",
    "        if start >= self.size():\n",
    "            return []\n",
    "        if end <= start or end <= 0:\n",
    "            return []\n",
    "        if end - start > self.size():\n",
    "            return self\n",
    "        if self.head == None:\n",
    "            raise Exception(\"ll is empty!\")\n",
    "        nodes = []\n",
    "        # traverse to node at start\n",
    "        cur = self.head\n",
    "        first = start\n",
    "        counter = end - start\n",
    "        while first > 0:\n",
    "            cur = cur.next\n",
    "            first -= 1\n",
    "        # traverse to node right before end and pick up nodes along\n",
    "        while counter > 0:\n",
    "            nodes.append(str(cur.data))\n",
    "            cur = cur.next\n",
    "            counter -= 1\n",
    "        return '->'.join(nodes)"
   ]
